{
  "name": "teamspirit",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "deploy": "gh-pages -d dist",
    "preview": "vite preview",
    "lint": "npx @biomejs/biome check src",
    "lint:fix": "npx @biomejs/biome check --write src",
    "lint:fix-unsafe": "npx @biomejs/biome check --write --unsafe src",
    "test": "vitest run",
    "type-check": "tsc -p tsconfig.json --noEmit",
    "prepare": "husky"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.0.0",
    "@types/node": "^24.0.3",
    "beercss": "^3.11.33",
    "gh-pages": "^6.2.0",
    "husky": "^9.1.7",
    "jsdom": "^26.0.0",
    "sass": "^1.81.0",
    "typescript": "^5.8.3",
    "vite": "^7.0.0",
    "vitest": "^3.0.4"
  }
}
